<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="oasis.aplicacion.controllerView.ViewAdministradorController">
   <children>
      <VBox layoutX="199.0" layoutY="100.0" prefHeight="200.0" prefWidth="203.0">
         <children>
            <Button fx:id="idBottonRegistroMedico" mnemonicParsing="false" onAction="#abrirRegistroMedico" prefHeight="25.0" prefWidth="210.0" text="Registrar Medico">
               <VBox.margin>
                  <Insets bottom="6.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="idBottonRegistroPaciente" mnemonicParsing="false" onAction="#abrirRegistroPaciente" prefHeight="25.0" prefWidth="210.0" text="Registrar Paciente">
               <VBox.margin>
                  <Insets bottom="6.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="idBottonAgendarCita" mnemonicParsing="false" onAction="#abrirCitaControlOValoracion" prefHeight="25.0" prefWidth="210.0" text="Agendar Cita">
               <VBox.margin>
                  <Insets bottom="6.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="idBottonAutorizarExamen" mnemonicParsing="false" prefHeight="25.0" prefWidth="210.0" text="Autorizar Examen">
               <VBox.margin>
                  <Insets bottom="6.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="idBottonPagarCita" mnemonicParsing="false" prefHeight="25.0" prefWidth="210.0" text="Pagar Cita">
               <VBox.margin>
                  <Insets bottom="6.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="idBottonModificarCita" mnemonicParsing="false" prefHeight="25.0" prefWidth="210.0" text="Modificar Cita">
               <VBox.margin>
                  <Insets bottom="6.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="idBottonCancelarCita" mnemonicParsing="false" prefHeight="25.0" prefWidth="210.0" text="Cancelar Cita">
               <VBox.margin>
                  <Insets bottom="6.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="idBottonEnviarSalaEspera" mnemonicParsing="false" prefHeight="25.0" prefWidth="210.0" text="Enviar a sala de espera">
               <VBox.margin>
                  <Insets bottom="6.0" />
               </VBox.margin>
            </Button>
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </VBox>
   </children>
</AnchorPane>
